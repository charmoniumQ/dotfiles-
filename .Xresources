! scroll back to the bottom on keypress
urxvt*scrollTtyKeypress: true

! scrollback buffer lines: delegate to tmux
urxvt*saveLines: 0

urxvt*font: xft:Liberation Mono:pixelsize=14

! Fullscreen switch
urxvt.perl-ext-common:  fullscreen
urxvt.keysym.F11:       perl:fullscreen:switch

! Composite transparency
! https://ctkarch.org/documentation/tutorials/tuto.php?page=urxvt-transpa.xml
urxvt*depth: 32
urxvt*background: rgba:1A00/1800/1700/c000
!urxvt*foreground: white

